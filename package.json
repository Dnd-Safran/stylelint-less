{
    "name": "stylelint-less",
    "version": "1.0.0",
    "description": "A collection of Less specific rules for stylelint",
    "babel": {
        "presets": [
            "@babel/env"
        ]
    },
    "main": "dist/index.js",

    "scripts": {
        "prebuild": "rimraf lib",
        "build": "babel src --out-dir lib"
    },

    "dependencies": {
        "stylelint": "^13.8.0",
        "postcss": "^6.0.22",
        "postcss-value-parser": "4.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-preset-jest": "^25.5.0"
    },
    "author": {
        "name": "Sivantarajan",
        "url": "https://github.com/ssivanatarajan/"
    },
    "bugs": {
        "url": "https://github.com/ssivanatarajan/stylelint-less/issues"
    },
    "homepage": "https://github.com/ssivanatarajan/stylelint-less#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ssivanatarajan/stylelint-less.git"
    },
    "license": "MIT",
    "env": {
        "es6": true
    },
    "keywords": [
        "stylelint",
        "stylelint-plugin",
        "css",
        "less"
    ]
}